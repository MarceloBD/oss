{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "function-paren-newline": 0,
    "no-confusing-arrow": 0,
    "object-curly-newline": 0,
    "import/no-named-as-default": 0,
    "arrow-parens": 0,
    "implicit-arrow-linebreak": 0,
    "max-len": ["error", { "code": 120 }],
    "import/prefer-default-export": 0
  },
  "env": {
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "node": true
    }
  }
}

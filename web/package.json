{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "getschema": "./node_modules/.bin/babel-node src/utils/relay/getSchema.js",
    "update": "yarn getschema && yarn relay &",
    "relay": "relay-compiler --src ./src --schema schema.graphql --watchman=false",
    "start-dev": "./node_modules/.bin/env-cmd -f ./.env.development ./node_modules/.bin/webpack-dev-server --hot --inline --disableHostCheck=true --mode=development --port 8081",
    "prebuild": "yarn --frozen-lockfile && rm -rf build && mkdir build && yarn relay",
    "build": " NODE_OPTIONS=--max_old_space_size=8192 ./node_modules/.bin/env-cmd .env.production ./node_modules/.bin/webpack --mode=production",
    "build-homolog": "rm -rf build && mkdir build && yarn relay && ./node_modules/.bin/env-cmd .env.homolog ./node_modules/.bin/webpack",
    "precommit": "lint-staged",
    "test": "./node_modules/.bin/eslint src/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "eslintConfig": {
    "globals": {
      "NODE_ENV": true
    }
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module",
      "options": {}
    }
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@material-ui/core": "^4.3.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "chalk": "^2.4.2",
    "extend": "^3.0.2",
    "farce": "^0.2.8",
    "file-loader": "^4.2.0",
    "found": "^0.4.7",
    "found-relay": "^0.4.1",
    "graphql": "^14.5.3",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "moment-timezone": "^0.5.26",
    "node-fetch": "^2.6.0",
    "ppath": "^1.0.0",
    "prop-types": "^15.7.2",
    "pubsub-js": "^1.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dotdotdot": "^1.3.1",
    "react-hot-loader": "^4.12.12",
    "react-redux": "7.0.0",
    "react-relay": "^5.0.0",
    "redux": "^4.0.4",
    "relay-runtime": "5.0.0",
    "styled-components": "^4.3.2",
    "universal-cookie": "^4.0.2",
    "webpack": "^4.39.3",
    "webpack-shell-plugin": "^0.5.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-transform-react-jsx-source": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-relay": "^5.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^5.2.0",
    "env-cmd": "^9.0.3",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "prettier": "^1.18.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}

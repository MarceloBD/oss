{
  "name": "moody",
  "version": "0.0.1",
  "description": "NewLaw postgrad portal",
  "private": true,
  "scripts": {
    "start": "yarn update && yarn start-dev",
    "getschema": "./node_modules/.bin/babel-node src/utils/relay/getSchema.js",
    "update": "yarn getschema && yarn relay &",
    "relay": "relay-compiler --src ./src --schema schema.graphql --watchman=false",
    "start-dev": "./node_modules/.bin/env-cmd .env.development ./node_modules/.bin/webpack-dev-server --hot --inline --disableHostCheck=true --mode=development --port 8081",
    "prebuild": "yarn --frozen-lockfile && rm -rf build && mkdir build && yarn relay",
    "build": " NODE_OPTIONS=--max_old_space_size=8192 ./node_modules/.bin/env-cmd .env.production ./node_modules/.bin/webpack --mode=production",
    "build-homolog": "rm -rf build && mkdir build && yarn relay && ./node_modules/.bin/env-cmd .env.homolog ./node_modules/.bin/webpack",
    "precommit": "lint-staged",
    "test": "./node_modules/.bin/eslint src/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "eslintConfig": {
    "globals": {
      "NODE_ENV": true
    }
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module",
      "options": {}
    }
  },
